<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>MODEL.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarUsuarioServlet</servlet-name>
        <servlet-class>MODEL.AgregarUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultarUsuarioServlet</servlet-name>
        <servlet-class>MODEL.ConsultarUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActualizarUsuarioServlet</servlet-name>
        <servlet-class>MODEL.ActualizarUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DarDeBajaUsuarioServlet</servlet-name>
        <servlet-class>MODEL.DarDeBajaUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarRolesServlet</servlet-name>
        <servlet-class>MODEL.ModificarRolesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarUsuarioServlet</servlet-name>
        <url-pattern>/AgregarUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultarUsuarioServlet</servlet-name>
        <url-pattern>/ConsultarUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActualizarUsuarioServlet</servlet-name>
        <url-pattern>/ActualizarUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DarDeBajaUsuarioServlet</servlet-name>
        <url-pattern>/DarDeBajaUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarRolesServlet</servlet-name>
        <url-pattern>/ModificarRolesServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
