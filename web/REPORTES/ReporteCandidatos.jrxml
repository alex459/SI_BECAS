INS.NOMBRE_INSTITUCION AS NOMBRE_INSTITUCION, PR.NOMBRE_PROGRESO AS NOMBRE_PROGRESO
FROM DETALLE_USUARIO DU, FACULTAD FA, OFERTA_BECA OB,
